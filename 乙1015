/*一类：d>h&&c>h
二类：d>h&&l<c<h
三类：l<d<h&&l<c<h&&d>c
四类：l<d<h&&l<c<h&&d<c||l<d<h&&c>h（即l<d<h&&d<c) 
五类：任何一门小于l的*/
#include<stdio.h>
#include<stdlib.h>
typedef struct student{
	int num;
	int d,c,lv,sum;
}stu;
int cmp(const void*a,const void*b)
/*哭了，我也不知道这函数在做什么
目测是说在结构体里按什么原则比较，
a指针的内容比b指针的小时，*a-*b:小的在前；*b-*a：大的在前， 
比较到差为零返回0为止*/ 
{
	stu head,tail;
	int ret=1;
	head=*(stu*)a;
	tail=*(stu*)b;
	if(head.lv!=tail.lv)
		ret=head.lv-tail.lv;//lv值低的在前 
	else if(head.sum!=tail.sum)
		ret=tail.sum-head.sum;//总分高的在前 
	else if(head.d!=tail.d) 
		ret=tail.d-head.d;
	else if(head.num!=tail.num)
		ret=head.num-tail.num;
	return ret;
}
int main()
{
	int n,l,h,i=0;
	int m[5]={0};
	scanf("%d %d %d",&n,&l,&h);
	stu s[n];
	while(i<n)
	{
		stu *p=&s[i++];
		scanf("%i %i %i",&(*p).num,&(*p).d,&(*p).c);
		p->sum=p->c+p->d;
		if(p->d<l||p->c<l)//德才任意一项不达标直接末等 
		{
			p->lv=4;
		}
		else if(p->d>=h)
		{
			if(p->c>=h) 
			{
				p->lv=0;
			}
			else p->lv=1;
		}
		else if(p->c>p->d)//不知道德才相等会怎样
		{
			p->lv=3;
		}
		else
			p->lv=2;
		m[p->lv]++;//等级计数君 
	}
	printf("%d\n",n-m[4]);
	qsort(s,n,sizeof(stu),cmp);
	for(i=0;i<n-m[4];i++)
	{
		printf("%08i %i %i",s[i].num,s[i].d,s[i].c);//以防准考证号高位为零
		if(i<n-m[4]-1)
			printf("\n");
	}
	return 0;
}
